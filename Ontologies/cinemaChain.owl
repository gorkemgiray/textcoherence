                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasCertificate"/>
                        <owl:hasValue rdf:resource="&cinemaChain;15"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;ApprovedRatingFilm"/>
        <rdfs:comment
