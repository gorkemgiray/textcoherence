    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#MatineeScreening -->

    <owl:Class rdf:about="&cinemaChain;MatineeScreening">
        <rdfs:label
            >A Matinee Screening represents a Screening before Noon, 1200 Hours, for a Film certified as suitable for viewing by customers aged 12 and younger.</rdfs:label>
        <rdfs:label
            >MatineeScreening</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;Screening"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasScreeningStartTime"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
