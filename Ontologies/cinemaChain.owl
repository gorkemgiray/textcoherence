    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#12ARatedFilm -->

    <owl:Class rdf:about="&cinemaChain;12ARatedFilm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;ApprovedRatingFilm"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasCertificate"/>
                        <owl:hasValue rdf:resource="&cinemaChain;12A"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;ApprovedRatingFilm"/>
        <rdfs:comment
            >A Film given a 12A Rating</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#12RatedFilm -->

    <owl:Class rdf:about="&cinemaChain;12RatedFilm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;ApprovedRatingFilm"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasCertificate"/>
                        <owl:hasValue rdf:resource="&cinemaChain;12"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;ApprovedRatingFilm"/>
        <rdfs:comment
            >A Film given a 12 Rating</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#15RatedFilm -->

