    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#ActionDoubleBillTicket -->

    <owl:Class rdf:about="&cinemaChain;ActionDoubleBillTicket">
        <rdfs:label
            >ActionDoubleBillTicket</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;DoubleBillTicket"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasScreeningAllowed"/>
                        <owl:onClass>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&cinemaChain;Screening"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&cinemaChain;hasFilm"/>
                                        <owl:onClass>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="&cinemaChain;Film"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="&cinemaChain;hasGenre"/>
                                                        <owl:onClass>
                                                            <owl:Class>
                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                    <rdf:Description rdf:about="&cinemaChain;Genre"/>
                                                                    <owl:Restriction>
                                                                        <owl:onProperty rdf:resource="&cinemaChain;filmGenre"/>
                                                                        <owl:hasValue>Action</owl:hasValue>
                                                                    </owl:Restriction>
                                                                </owl:intersectionOf>
                                                            </owl:Class>
                                                        </owl:onClass>
                                                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:onClass>
                                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:onClass>
                        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;DoubleBillTicket"/>
        <rdfs:comment
            >ActionDoubleBillTicket represents a class for a DoubleBill Screening, where Films with Action Genre are shown one after another on one Ticket, where Ticket is for 1 Seat and has 1 Price.</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#Address -->

    <owl:Class rdf:about="&cinemaChain;Address">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;addressLine"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;city"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;postcode"/>
                        <owl:someValuesFrom rdf:resource="&xsd;string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;Location"/>
        <rdfs:comment
            >An Address for a cinema location. Contains a first line of the address, the city and the post code</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#Adult -->

    <owl:Class rdf:about="&cinemaChain;Adult">
        <rdfs:label xml:lang="en">Adult</rdfs:label>
        <rdfs:subClassOf rdf:resource="&cinemaChain;Customer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cinemaChain;customerAge"/>
                <owl:someValuesFrom>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdfs;Datatype"/>
                        <owl:onDatatype rdf:resource="&xsd;integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="&xsd;integer">26</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="&xsd;integer">69</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdf:Description>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >Adult Customer - a Customer whose Age is between 26 and 69</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#ApprovedRatingFilm -->

    <owl:Class rdf:about="&cinemaChain;ApprovedRatingFilm">
        <rdfs:subClassOf rdf:resource="&cinemaChain;Film"/>
        <rdfs:comment
            >This is a superclass, where named subclasses have different film certificate ratings which define the age of Customers those Films are suitable for</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#Assistant -->

    <owl:Class rdf:about="&cinemaChain;Assistant">
        <rdfs:subClassOf rdf:resource="&cinemaChain;Staff"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cinemaChain;hasManager"/>
                <owl:someValuesFrom rdf:resource="&cinemaChain;Manager"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&cinemaChain;Manager"/>
        <rdfs:comment xml:lang="en"
            >Assistant is an employee of the cinema, and has a Manager. Since hasManager object property is transitive, they have some manager which will include a direct manager and any transitive managers.</rdfs:comment>
        <rdfs:comment xml:lang="en">Employee</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#Auditorium -->

    <owl:Class rdf:about="&cinemaChain;Auditorium">
        <rdfs:label xml:lang="en">Auditorium</rdfs:label>
        <rdfs:subClassOf rdf:resource="&cinemaChain;Location"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cinemaChain;isAuditoriumOf"/>
                <owl:onClass rdf:resource="&cinemaChain;Cinema"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&cinemaChain;hasAuditoriumCapacity"/>
                <owl:onClass rdf:resource="&cinemaChain;AuditoriumCapacity"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en"
            >An Auditorium is linked to a exactly one Cinema. An Auditorium hosts Screenings of Films.</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#AuditoriumCapacity -->

