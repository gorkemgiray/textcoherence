<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY cinemaChain "http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#" >
    <!ENTITY cinemaChain3 "http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#12" >
    <!ENTITY cinemaChain4 "http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#15" >
    <!ENTITY cinemaChain2 "http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#18" >
]>


<rdf:RDF xmlns="http://users.ecs.soton.ac.uk/~cm7e09/files/cinemaChain.owl#"
     xml:base="http://users.ecs.soton.ac.uk/~cm7e09/files/cinemaChain.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cinemaChain4="&cinemaChain;15"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:cinemaChain2="&cinemaChain;18"
     xmlns:cinemaChain3="&cinemaChain;12"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:cinemaChain="http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="">
        <rdfs:comment
            >An ontology for a cinema chain. The Cinema has Auditoriums that show screenings of Films. In addition the Cinema Chain ontology also contains Staff and Car Park entities, which have been populated with instance data and tested to work, as it would allow for interesting queries such as returning Cinemas that have Free Parking and some Disabled Parking Spaces.

This Cinema Chain ontology could be extended to include all things normally found in a cinema chain, such as Food and Drink for sale.</rdfs:comment>
        <dc:date>2010-04-19</dc:date>
        <dc:creator
            >c. f. mccabe, email: cm7e09@ecs.soton.ac.uk</dc:creator>
        <dc:rights
            >Licensed under Creative Commons license:
cc-sa
Share Alike</dc:rights>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#contains -->

    <owl:ObjectProperty rdf:about="&cinemaChain;contains">
        <rdfs:comment
            >Contains other entities. In this case the domain and range defines that Groups can contain Organisations</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Group"/>
        <rdfs:range rdf:resource="&cinemaChain;Organisation"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isContainedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasAddress -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasAddress">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >This is the address location of the cinema</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Address"/>
        <rdfs:domain rdf:resource="&cinemaChain;Cinema"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasAuditorium -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasAuditorium">
        <rdfs:comment
            >An auditorium hosts the screenings for a cinema. It belongs to exactly one cinema. There can be multiple auditoriums within a cinema</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:domain rdf:resource="&cinemaChain;Cinema"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isAuditoriumOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasAuditoriumCapacity -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasAuditoriumCapacity">
        <rdfs:comment
            >The auditorium has a seating capacity</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:range rdf:resource="&cinemaChain;AuditoriumCapacity"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasAuditoriumScreening -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasAuditoriumScreening">
        <rdfs:comment
            >the auditorium that is showing the screening</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasCarPark -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasCarPark">
        <rdfs:comment
            >A cinema can have a car park</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;CarPark"/>
        <rdfs:domain rdf:resource="&cinemaChain;Cinema"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isCarParkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasCarParkCapacity -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasCarParkCapacity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The capacity of a car park, ie. the number of parking spaces it has</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set.</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;CarPark"/>
        <rdfs:range rdf:resource="&cinemaChain;CarParkCapacity"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isCarParkCapacityOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasCertificate -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasCertificate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >Each Film screened at a cinema has a certificate</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set.</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Certificate"/>
        <rdfs:domain rdf:resource="&cinemaChain;Film"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isCertificateOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasEmployee -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasEmployee">
        <rdfs:comment
            >Organisations like a cinema have employees</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Cinema"/>
        <rdfs:range rdf:resource="&cinemaChain;Staff"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isEmployeeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasFilm -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasFilm">
        <rdfs:comment
            >A screening has exactly one Film shown</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Film"/>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isFilmOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasFormat -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasFormat">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >The format of the Film screening. e.g Digital</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Format"/>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isFormatOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasGenre -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasGenre">
        <rdfs:comment
            >The genre of a film. Films can have one or more genres</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Film"/>
        <rdfs:range rdf:resource="&cinemaChain;Genre"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isGenreOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasManager -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasManager">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment
            >A transitive property. ie. if A hasManager B, and B hasManager C, then A hasManager C is true.</rdfs:comment>
        <rdfs:comment
            >Has manager describes that a person has a manager.</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Manager"/>
        <rdfs:domain rdf:resource="&cinemaChain;Staff"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isManagerOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasParkingPrice -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasParkingPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Pay Car Parks have a parking price</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;PayAndDisplay"/>
        <rdfs:range rdf:resource="&cinemaChain;Price"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasReservation -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasReservation">
        <rdfs:comment
            >Customer has a Reservation</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Customer"/>
        <rdfs:range rdf:resource="&cinemaChain;Reservation"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasScheduledScreening -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasScheduledScreening">
        <rdfs:comment
            >Cinemas have individual schedules of screenings of films.</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Cinema"/>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasScreeningAllowed -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasScreeningAllowed">
        <rdfs:comment
            >Each ticket will be for one specific screening allowed only</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
        <rdfs:domain rdf:resource="&cinemaChain;Ticket"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasScreeningTicket"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasScreeningDate -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasScreeningDate">
        <rdfs:comment
            >A date of a screening</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Date"/>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasScreeningStartTime -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasScreeningStartTime">
        <rdfs:comment
            >The start time for a Screening</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <rdfs:range rdf:resource="&cinemaChain;Time"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasScreeningTicket -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasScreeningTicket">
        <rdfs:comment
            >Each screening in a cinema requires its own type of ticket</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <rdfs:range rdf:resource="&cinemaChain;Ticket"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasSeat -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasSeat">
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:comment
            >Each ticket has exactly one seat.</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Seat"/>
        <rdfs:domain rdf:resource="&cinemaChain;Ticket"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isSeatOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasTicketPrice -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasTicketPrice">
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:comment
            >Each ticket has a price</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Price"/>
        <rdfs:domain rdf:resource="&cinemaChain;Ticket"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isTicketPriceOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#hasTicketReservation -->

    <owl:ObjectProperty rdf:about="&cinemaChain;hasTicketReservation">
        <rdfs:comment
            >A reservation for a ticket</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Reservation"/>
        <rdfs:range rdf:resource="&cinemaChain;Ticket"/>
        <owl:inverseOf rdf:resource="&cinemaChain;isTicketReservationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isAddressOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isAddressOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:comment
            >An address location belongs to a single cinema</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Address"/>
        <rdfs:range rdf:resource="&cinemaChain;Cinema"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasAddress"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isAuditoriumCapacityOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isAuditoriumCapacityOf">
        <rdfs:comment
            >The capacity of a particular auditorium</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:domain rdf:resource="&cinemaChain;AuditoriumCapacity"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasAuditoriumCapacity"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isAuditoriumOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isAuditoriumOf">
        <rdfs:comment
            >An auditorium will belong to exactly one cinema</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:range rdf:resource="&cinemaChain;Cinema"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isCarParkCapacityOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isCarParkCapacityOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The parking capacity of a car park. ie. the number of parking spaces belonging to a car park</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;CarPark"/>
        <rdfs:domain rdf:resource="&cinemaChain;CarParkCapacity"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isCarParkOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isCarParkOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:comment
            >The car park will belong to a single cinema</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;CarPark"/>
        <rdfs:range rdf:resource="&cinemaChain;Cinema"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isCertificateOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isCertificateOf">
        <rdfs:comment
            >The certificate belonging to a film</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Certificate"/>
        <rdfs:range rdf:resource="&cinemaChain;Film"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isContainedBy -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isContainedBy">
        <rdfs:comment
            >is contained by an entity.</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Group"/>
        <rdfs:range rdf:resource="&cinemaChain;Group"/>
        <rdfs:domain rdf:resource="&cinemaChain;Organisation"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isEmployeeOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isEmployeeOf">
        <rdfs:comment
            >an employee of the cinema</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Cinema"/>
        <rdfs:domain rdf:resource="&cinemaChain;Staff"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isFilmOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isFilmOf">
        <rdfs:comment
            >A film screening is for a particular film</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Film"/>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isFormatOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isFormatOf">
        <rdfs:comment
            >Each screening of a film has a particular format</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Format"/>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isGenreOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isGenreOf">
        <rdfs:comment
            >A film genre belonging to a Film</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Film"/>
        <rdfs:domain rdf:resource="&cinemaChain;Genre"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isManagerOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isManagerOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment
            >isManagerOf is describes that a person is a manager of another person</rdfs:comment>
        <rdfs:comment
            >A transitive property. ie. if A isManagerOf B, and B isManagerOf C, then A isManagerOf C also.</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Manager"/>
        <rdfs:range rdf:resource="&cinemaChain;Staff"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isParkingPriceOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isParkingPriceOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Price of a Pay and Display Car Park</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;PayAndDisplay"/>
        <rdfs:domain rdf:resource="&cinemaChain;Price"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasParkingPrice"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isReservationOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isReservationOf">
        <rdfs:comment
            >A reservation of a specific Customer</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Customer"/>
        <rdfs:domain rdf:resource="&cinemaChain;Reservation"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasReservation"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isScreeningDateOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isScreeningDateOf">
        <rdfs:comment
            >the screening date belonging of a Screening</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Date"/>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasScreeningDate"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isScreeningInAuditoriumOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isScreeningInAuditoriumOf">
        <rdfs:comment
            >The Auditorium in which a particular screening is sheduled.</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Auditorium"/>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasAuditoriumScreening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isScreeningInCinemaOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isScreeningInCinemaOf">
        <rdfs:comment
            >A Screening in a particular cinema</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Cinema"/>
        <rdfs:domain rdf:resource="&cinemaChain;Screening"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasScheduledScreening"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isScreeningStartTimeOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isScreeningStartTimeOf">
        <rdfs:comment
            >The start time belonging to a screening</rdfs:comment>
        <rdfs:range rdf:resource="&cinemaChain;Screening"/>
        <rdfs:domain rdf:resource="&cinemaChain;Time"/>
        <owl:inverseOf rdf:resource="&cinemaChain;hasScreeningStartTime"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isSeatOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isSeatOf">
        <rdfs:comment
            >The seat assigned to a ticket</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Seat"/>
        <rdfs:range rdf:resource="&cinemaChain;Ticket"/>
    </owl:ObjectProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#isTicketPriceOf -->

    <owl:ObjectProperty rdf:about="&cinemaChain;isTicketPriceOf">
        <rdfs:comment
