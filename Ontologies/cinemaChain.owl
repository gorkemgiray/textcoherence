        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >the Postcode forms part of an Address and is usually an alphanumeric string, eg. SO17 1UR</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Address"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#rowNumber -->

    <owl:DatatypeProperty rdf:about="&cinemaChain;rowNumber">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
