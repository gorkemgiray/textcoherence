                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&cinemaChain;Film"/>
        <rdfs:comment xml:lang="en"
            >An average duration film is in the range of 20 minutes up to and including 89 minutes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#CarPark -->

    <owl:Class rdf:about="&cinemaChain;CarPark">
