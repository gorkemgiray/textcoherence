    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#runningTime -->

    <owl:DatatypeProperty rdf:about="&cinemaChain;runningTime">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >The running time is the duration of the Film measured in minutes</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Film"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#seatNumber -->

    <owl:DatatypeProperty rdf:about="&cinemaChain;seatNumber">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
