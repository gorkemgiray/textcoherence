    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#18RatedFilm -->

    <owl:Class rdf:about="&cinemaChain;18RatedFilm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;ApprovedRatingFilm"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;hasCertificate"/>
                        <owl:hasValue rdf:resource="&cinemaChain;18"/>
