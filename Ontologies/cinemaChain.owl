            <owl:Restriction>
                <owl:onProperty rdf:resource="&cinemaChain;numberOfSeats"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="&xsd;integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >The Auditorium Capacity describes the number of Seats it contains, and thus the maximum number of Customers who can watch a particular Screening of a Film.</rdfs:comment>
    </owl:Class>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#AverageFilm -->

    <owl:Class rdf:about="&cinemaChain;AverageFilm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cinemaChain;Film"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&cinemaChain;runningTime"/>
                        <owl:someValuesFrom>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdfs;Datatype"/>
                                <owl:onDatatype rdf:resource="&xsd;integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="&xsd;integer">20</xsd:minInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="&xsd;integer">90</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdf:Description>
                        </owl:someValuesFrom>
