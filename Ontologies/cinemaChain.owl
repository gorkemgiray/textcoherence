        <rdfs:domain rdf:resource="&cinemaChain;Seat"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#smallUnitCurrency -->

    <owl:DatatypeProperty rdf:about="&cinemaChain;smallUnitCurrency">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >Small Unit Currency describes the smallest unit of currency to be used in Pricing. ie. the larger denomination</rdfs:comment>
        <rdfs:comment
            >Functional property, can only have one property set for any instance</rdfs:comment>
        <rdfs:domain rdf:resource="&cinemaChain;Price"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://users.ecs.soton.ac.uk/~cm7e09/ontology/cinemaChain.owl#suitableFor -->

    <owl:DatatypeProperty rdf:about="&cinemaChain;suitableFor">
        <rdfs:label>suitableForAge</rdfs:label>
