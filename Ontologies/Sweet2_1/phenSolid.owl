<!DOCTYPE rdf:RDF [
  <!ENTITY solid "http://sweet.jpl.nasa.gov/2.1/phenSolid.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.1/statePhysical.owl">
  <!ENTITY proc "http://sweet.jpl.nasa.gov/2.1/procPhysical.owl">
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.1/reprSpaceRelation.owl">
  <!ENTITY phen "http://sweet.jpl.nasa.gov/2.1/phen.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
