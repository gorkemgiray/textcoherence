<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY reac "http://sweet.jpl.nasa.gov/2.1/phenReaction.owl">
  <!ENTITY cproc "http://sweet.jpl.nasa.gov/2.1/procChemical.owl">
  <!ENTITY wave "http://sweet.jpl.nasa.gov/2.1/phenWave.owl">
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.1/stateRole.owl">
  <!ENTITY impa "http://sweet.jpl.nasa.gov/2.1/stateRoleImpact.owl">
  <!ENTITY phen "http://sweet.jpl.nasa.gov/2.1/phen.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&reac;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:reac="&reac;"
         xmlns:role="&role;"
         xmlns:phen="&phen;"
         xmlns:impa="&impa;"
         xmlns:cproc="&cproc;"
         xmlns:wave="&wave;"
>

<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.1">
     <owl:imports rdf:resource="&cproc;"/>
     <owl:imports rdf:resource="&wave;"/>
     <owl:imports rdf:resource="&role;"/>
     <owl:imports rdf:resource="&impa;"/>
     <owl:imports rdf:resource="&phen;"/>
  </owl:Ontology>

<!-- Phenomena -->
  <owl:Class rdf:about="#Explosion">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&impa;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Disaster" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&phen;#hasPhenomena" />
        <owl:allValuesFrom rdf:resource="&wave;#Noise" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Fire">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&impa;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Disaster" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Flare">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReaction"/>
    <owl:equivalentClass rdf:resource="#Flaring"/>
  </owl:Class>
  <owl:Class rdf:about="#Flaring"/>

  <owl:Class rdf:about="#FluidLeak">
    <rdfs:subClassOf rdf:resource="#Leak"/>
  </owl:Class>

  <owl:Class rdf:about="#GasFlare">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&impa;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Disaster" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#GasLeak">
    <rdfs:subClassOf rdf:resource="#Leak"/>
  </owl:Class>

  <owl:Class rdf:about="#Emission">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReaction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&role;#hasRole" />
        <owl:hasValue rdf:resource="&role;#ByProduct" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Eruption">
    <rdfs:subClassOf rdf:resource="#Release"/>
  </owl:Class>

  <owl:Class rdf:about="#IndustrialEmission">
    <rdfs:subClassOf rdf:resource="#Emission"/>
  </owl:Class>

  <owl:Class rdf:about="#Leak">
    <rdfs:subClassOf rdf:resource="#Release"/>
  </owl:Class>

  <owl:Class rdf:about="#Release">
    <rdfs:subClassOf rdf:resource="#Emission"/>
  </owl:Class>

  <owl:Class rdf:about="#BiochemicalRelease">
    <rdfs:subClassOf rdf:resource="#Release"/>
  </owl:Class>

</rdf:RDF>
