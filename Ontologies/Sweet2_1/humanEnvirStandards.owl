  </owl:Class>

  <owl:Class rdf:about="#NewSourcePerformanceStandard">
    <rdfs:subClassOf rdf:resource="#EnvironmentalStandard"/>
    <owl:equivalentClass rdf:resource="#NSPS"/>
  </owl:Class>
  <owl:Class rdf:about="#NSPS"/>

