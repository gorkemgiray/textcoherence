  <owl:Class rdf:about="#REC"/>

  <owl:Class rdf:about="#ReferenceExposureLevel">
    <rdfs:subClassOf rdf:resource="#EnvironmentalStandard"/>
    <owl:equivalentClass rdf:resource="#REL"/>
