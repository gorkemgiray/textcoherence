  </owl:Class>

  <owl:Class rdf:about="#EnvironmentalStandardsBody">
    <rdfs:subClassOf rdf:resource="&jur;StandardsBody"/>
  </owl:Class>

  <owl:Class rdf:about="#NonAttainmentArea">
